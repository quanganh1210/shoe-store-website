@model ThietKeWebBTL_User2.Models.GenderProduct

@{
	ViewData["Title"] = "AddGenderProduct";
}

<div class="card">
	<div class="card-body">
		<h1>Add GenderProduct</h1>
		<hr />
		<div class="row">
			<div class="col-md-4">
				<form asp-action="AddGenderProduct">
					<div asp-validation-summary="ModelOnly" class="text-danger"></div>
					<div class="form-group">
						<label asp-for="GenderId" class="control-label"></label>
						<select asp-for="GenderId" class="form-control" asp-items="ViewBag.GenderId"></select>
					</div>
					<div class="form-group">
						<label asp-for="ProductId" class="control-label"></label>
						<select asp-for="ProductId" class="form-control chosen-select" asp-items="ViewBag.ProductId"></select>
					</div>
					<div class="form-group">
						<input type="submit" value="Create" class="btn btn-primary" />
					</div>
				</form>
			</div>
		</div>

		<div>
			<a asp-action="GenderProductTable">Back to List</a>
		</div>
	</div>
</div>
@section Scripts{
	@{
		<partial name="_ValidationScriptsPartial" />
	}
	}

	

